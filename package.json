{
  "name": "common-util",
  "private": true,
  "version": "0.0.34",
  "description": "常用工具",
  "license": "MIT",
  "bin": {
    "mock-server": "bin/mock-server.js",
    "gen-mock": "bin/gen-mock.js"
  },
  "scripts": {
    "test": "jest",
    "test:detail": "jest --coverage --verbose",
    "dev": "pnpm -C play dev",
    "gen": "bash ./scripts/gc.sh",
    "clean": "pnpm run clean:lib && pnpm run clean -r --stream",
    "clean:lib": "rimraf dist",
    "build": "gulp --require sucrase/register/ts -f build/gulpfile.ts",
    "mock-server": "node ./bin/mock-server.js"
  },
  "workspaces": [
    "packages/*"
  ],
  "pre-commit": [
    "test"
  ],
  "files": [
    "dist"
  ],
  "dependencies": {
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "body-parser": "^1.19.0",
    "chokidar": "^3.5.1",
    "express": "^4.17.1",
    "ramda": "^0.27.1",
    "rollup": "^2.60.0",
    "rollup-plugin-esbuild": "4.6.0"
  },
  "devDependencies": {
    "@pnpm/find-workspace-packages": "3.1.23",
    "@pnpm/logger": "4.0.0",
    "@pnpm/types": "7.6.0",
    "@rollup/plugin-replace": "3.0.0",
    "@sucrase/jest-plugin": "2.2.0",
    "@types/gulp": "4.0.9",
    "@types/jest": "27.0.2",
    "@types/ramda": "^0.27.36",
    "@types/through2": "2.0.36",
    "@typescript-eslint/eslint-plugin": "5.4.0",
    "@typescript-eslint/parser": "5.4.0",
    "chalk": "4.1.2",
    "components-helper": "1.0.4",
    "cz-conventional-changelog": "3.3.0",
    "esbuild": "0.13.14",
    "eslint": "8.2.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-define-config": "1.1.3",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-prettier": "4.0.0",
    "expect-type": "0.13.0",
    "fast-glob": "3.2.7",
    "gulp": "4.0.2",
    "gulp-typescript": "6.0.0-alpha.1",
    "intersection-observer": "^0.12.0",
    "jest": "26.6.3",
    "lint-staged": "11.2.6",
    "prettier": "2.4.1",
    "pretty-quick": "3.1.2",
    "resize-observer-polyfill": "1.5.1",
    "rimraf": "3.0.2",
    "rollup-plugin-css-only": "3.1.0",
    "rollup-plugin-esbuild": "4.6.0",
    "rollup-plugin-filesize": "9.1.1",
    "sucrase": "3.20.3",
    "through2": "4.0.2",
    "ts-jest": "26.5.6",
    "ts-morph": "12.2.0",
    "typescript": "4.5.2"
  },
  "peerDependencies": {
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "body-parser": "^1.19.0",
    "chokidar": "^3.5.1",
    "express": "^4.17.1",
    "ramda": "^0.27.1",
    "rollup": "^2.60.0",
    "rollup-plugin-esbuild": "4.6.0"
  },
  "exports": {
    ".": {
      "require": "./dist/lib/index.js",
      "import": "./dist/es/index.mjs"
    },
    "./es": "./dist/es/index.mjs",
    "./lib": "./dist/lib/index.js",
    "./*": "./dist/*"
  },
  "sideEffects": [
    "dist/*"
  ],
  "main": "dist/lib/index.js",
  "module": "dist/es/index.mjs",
  "unpkg": "dist/index.full.js",
  "jsdelivr": "dist/index.full.js",
  "engines": {
    "node": ">= 16"
  },
  "browserslist": [
    "> 1%",
    "not ie 11",
    "not op_mini all"
  ]
}
